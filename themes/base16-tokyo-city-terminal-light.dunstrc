# Template author: Tinted Theming (https://github.com/tinted-theming)

[global]
    frame_color = "#28323a"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#34548a"
    foreground = "#d8e2ec"

[base16_normal]
    msg_urgency = normal
    background = "#fbfbfd"
    foreground = "#28323a"

[base16_critical]
    msg_urgency = critical
    background = "#8c4351"
    foreground = "#d8e2ec"
